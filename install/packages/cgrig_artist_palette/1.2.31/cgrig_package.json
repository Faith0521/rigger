{
    "author": "Create3dCharacters",
    "authorEmail": "info@create3dcharacters.com",
    "commands": [
        "{self}/scripts/cgrig_artist_palette_startup.py"
    ],
    "description": "Maya tool, menu and shelf related code for CgRig Tools Pro",
    "displayName": "CgRig Artist Palette",
    "documentation": {
        "masterDoc": "index.rst",
        "sourceCodeFolder": "{self}/cgrig",
        "sourceFolder": "{self}/docs/source"
    },
    "environment": {
        "PYTHONPATH": [
            "{self}"
        ],
        "CGRIG_ACTIONPLUGIN_MODULES": [
            "{self}/cgrig/apps/toolpalette/default_actions.py"
        ],
        "CGRIG_ARTIST_PALETTE_PATH": [
            "{self}/cgrig/apps/toolpalette/palette.py"
        ],
        "CGRIG_BASE_PATHS": [
            "{self}"
        ],
        "CGRIG_ICON_PATHS": [
            "{self}/icons"
        ],
        "CGRIG_SHELF_LAYOUTS": [
            "{self}/cgrig/apps/toolpalette/generic_shelf.layout",
            "{self}/cgrig/apps/toolpalette/animcustomshelf.layout"
        ]
    },
    "name": "cgrig_artist_palette",
    "requirements": [
        "cgrig_core",
        "cgrig_maya"
    ],
    "tests": [
        "{self}/tests"
    ],
    "version": "1.2.31"
}